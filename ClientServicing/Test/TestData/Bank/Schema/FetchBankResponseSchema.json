{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Fetch Banks Response",
    "type": "object",
    "properties": {
        "succeeded": { "type": "boolean" },
        "message": { "type": [ "string", "null" ] },
        "errors": { "type": [ "string", "null" ] },
        "data": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "bankID": { "type": "integer" },
                    "bankName": { "type": "string" },
                    "bankShortName": { "type": [ "string", "null" ] },
                    "dispSeq": { "type": "integer" },
                    "isActive": { "type": "boolean" },
                    "lastChanged": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "userID": { "type": "string" }
                },
                "required": [
                    "bankID",
                    "bankName",
                    "bankShortName",
                    "dispSeq",
                    "isActive",
                    "lastChanged",
                    "userID"
                ],
                "additionalProperties": false
            }
        }
    },
    "required": [ "succeeded", "message", "errors", "data" ],
    "additionalProperties": false
}
